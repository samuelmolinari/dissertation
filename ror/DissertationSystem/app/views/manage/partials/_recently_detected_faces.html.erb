<section id="organizer-faces">
	<section id="organizer-faces-lists">
	<h2>Detected Faces</h2>
	<%- @user.detections.find(:all,:order => "id DESC", :limit => 24).each do |f| %>
		<div class="face-thumb"><%= link_to image_tag(f.access_control_url(60,60)),{:controller => :user, :action => :photo, :ref => f.photo.ref, :size => :medium} %></div>
	<%- end %>
	</section>
</section>